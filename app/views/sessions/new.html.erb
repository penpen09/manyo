<div class="form-signin">
<h1 class="form-signin-heading">ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email ,class:'sr-only'%>
  <%= f.email_field :email,class:'form-control',id:'inputEmail',placeholder:'Email address'%>
  <%= f.label :password ,class:'sr-only'%>
  <%= f.password_field :password,class:'form-control',id:'inputPassword',placeholder:'Password'%>
  <%= f.submit "Log in" ,class: 'btn btn-primary btn-block'%>
<% end %>
</div>
